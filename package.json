{
  "name": "ocr-parser",
  "version": "0.2.4",
  "description": "Fast and efficient DOM-less OCR parser for use in browsers (including Workers) and Node",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "ocr",
    "parser",
    "domless",
    "worker"
  ],
  "author": "Johannes Baiter <johannes.baiter@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "watch": "tsup src/index.ts --format esm,cjs --dts --sourcemap --watch",
    "pretest": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rm -rf dist",
    "format": "prettier --write src test",
    "docs": "typedoc src/index.ts",
    "prepack": "rm -rf dist && npm run test && npm run build"
  },
  "dependencies": {
    "sax-wasm": "2.2.4"
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "plugins": [
      "prettier-plugin-jsdoc"
    ]
  },
  "typedocOptions": {
    "out": "_site"
  }
}
